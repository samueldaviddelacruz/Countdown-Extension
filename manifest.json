{
  "manifest_version": 2,

  "name": "Countown counter",
  "description": "This extension shows a counter to a given event/goal",
  "version": "1.1",

  "browser_action": {
    "default_popup": "popup.html"
  },

  "chrome_url_overrides" : {
    "newtab": "popup.html"
  },

  "permissions": [
    "activeTab",
    "storage"
  ]
}